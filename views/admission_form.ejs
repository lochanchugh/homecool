	background-color: #2c6f77;
	border: none;
	padding: 0.5em;
	width: 9em;
	transition: 0.3s;
	margin: 0.2em;
	text-transform: uppercase;
	font-size: 1.2em;
	font-weight: bold;
}

button:hover{
	background-color: #000;
}

.input-bar{
	width: 100%;
	height: 1px;
	background-color: #555;
}

.progress-bar{
	width: 7%;
	height: 2px;
	background-color: #fff;
}

.steps-navigation{
	float: right;
	padding: 30px;
}

.current label{
	opacity: 1;
	transition: all 1s;
}

button{
  cursor: pointer;
}

@keyframes fadeIn {
	from{ 
		opacity:  0; 
	}
	to{
		opacity: 1;
	}
}
@keyframes fadeInOut {
	0%{ 
		opacity:  0; 
	}
	50%{
		opacity: 1;
	}
	100%{
		opacity: 0;
	}
}
a:link {
